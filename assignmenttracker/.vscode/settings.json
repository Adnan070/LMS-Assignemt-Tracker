{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "editor.formatOnPaste": false,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": false
    },
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "package-lock.json": false,
        "metro.config.js": false,
        "babel.config.js": false,
        ".watchmanconfig": false,
        "node_modules": false,
        ".flowconfig": false,
        "react-native.config.js": false,
        "android": false,
        "ios": false,
        "__tests__": false,
        ".gitignore": false,
        "app.json": false
    },
    "explorerExclude.backup": null
}
